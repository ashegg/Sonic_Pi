use_bpm 148
george = [1,1,1,5,1,1,1,3, 1,1,1,1,1,5,1,1,1,5]
i=0
j =
  0
kelly = [0.5, 0.6, 0.7, 0.8, 0.9]
k = [:eb4, :g4,:eb5,:d5,:eb4,:g4,:eb5, :d5,:bb4,:ab4,:eb4,:f4, :ab5,:g5, :f4, :ab5, :g5, :f5,]
z = 0
live_loop :kkkl do
  18.times do
    play (k [z])
    sleep (george [i])
    amp (kelly [j])
    z = z + 1
    i = i + 1
    j = j + 1
    print (z)
    print (i)
  end
  z = 0
  i =0
  j =0
end
